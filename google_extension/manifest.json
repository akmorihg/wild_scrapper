{
    "manifest_version": 3,
    "name": "Wild Scrapper",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["https://*/*", "http://*/*"],
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    }
}
  